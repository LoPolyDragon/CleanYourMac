# Duplicate file detection function
execute_duplicate_detection() {
    print_title "üîç ÈáçÂ§çÊñá‰ª∂Ê£ÄÊµã"
    echo ""
    
    echo "üîé Âø´ÈÄüÈáçÂ§çÊñá‰ª∂Ê£ÄÊµã..."
    echo "üìÅ Â∞ÜÊâ´ÊèèÔºöDocuments, Downloads, Desktop Êñá‰ª∂Â§π"
    echo ""
    
    # Create temporary files for processing
    local temp_dir="/tmp/cleanmac_duplicate_$$"
    mkdir -p "$temp_dir"
    local results_file="$temp_dir/results.txt"
    
    # Search for files in common directories
    local search_paths=("$HOME/Documents" "$HOME/Downloads" "$HOME/Desktop")
    local total_files=0
    local duplicate_count=0
    local duplicate_size_saved=0
    
    echo "üìä ÂàÜÊûêÊñá‰ª∂‰∏≠..."
    
    # Simple approach: find files by size first, then hash only potential duplicates
    for search_path in "${search_paths[@]}"; do
        if [[ -d "$search_path" ]]; then
            echo "  Ê£ÄÊü• $(basename "$search_path")..."
            
            # Find files and group by size (much faster than hashing everything)
            local size_groups=()
            while IFS= read -r -d '' file; do
                if [[ -f "$file" && -r "$file" ]]; then
                    local size=$(stat -f%z "$file" 2>/dev/null || echo "0")
                    if [[ $size -gt 1048576 && $size -lt 52428800 ]]; then  # 1MB to 50MB
                        echo "$size|$file" >> "$temp_dir/size_list.txt"
                        ((total_files++))
                    fi
                fi
                # Limit files to prevent hanging
                if [[ $total_files -gt 30 ]]; then
                    echo "    (ÈôêÂà∂Êâ´Êèè30‰∏™Êñá‰ª∂‰ª•ÊèêÈ´òÊÄßËÉΩ)"
                    break 2
                fi
            done < <(find "$search_path" -maxdepth 2 -type f -print0 2>/dev/null)
        fi
    done
    
    echo "  ÂÖ±ÊâæÂà∞ $total_files ‰∏™ÂÄôÈÄâÊñá‰ª∂"
    echo ""
    
    if [[ $total_files -eq 0 ]]; then
        echo "üìù Êú™ÊâæÂà∞ÈÄÇÂêàÊ£ÄÊµãÁöÑÊñá‰ª∂"
        echo "üí° ÈáçÂ§çÊñá‰ª∂Ê£ÄÊµãÈúÄË¶Å1MB-50MBÁöÑÊñá‰ª∂"
    elif [[ -f "$temp_dir/size_list.txt" ]]; then
        echo "üîç Êü•ÊâæÁõ∏ÂêåÂ§ßÂ∞èÁöÑÊñá‰ª∂..."
        
        # Find files with same size (potential duplicates)
        sort "$temp_dir/size_list.txt" | uniq -d -w 10 > "$temp_dir/same_size.txt"
        
        if [[ -s "$temp_dir/same_size.txt" ]]; then
            echo "‚ö†Ô∏è  ÂèëÁé∞Áõ∏ÂêåÂ§ßÂ∞èÁöÑÊñá‰ª∂ÔºàÂèØËÉΩÈáçÂ§çÔºâÔºö"
            echo ""
            
            local current_size=""
            local size_files=()
            local group_count=0
            
            while IFS='|' read -r file_size file_path; do
                if [[ "$file_size" != "$current_size" ]]; then
                    # Process previous group
                    if [[ ${#size_files[@]} -gt 1 ]]; then
                        ((group_count++))
                        local size_human=$(bytes_to_human $file_size)
                        echo "üìÑ Áõ∏ÂêåÂ§ßÂ∞èÁªÑ #$group_count ($size_human):"
                        for same_file in "${size_files[@]}"; do
                            echo "  ‚Ä¢ $(basename "$same_file")"
                            echo "    $same_file"
                            if [[ $duplicate_count -eq 0 ]]; then
                                duplicate_count=1  # First file of group
                            else
                                duplicate_size_saved=$((duplicate_size_saved + file_size))
                                ((duplicate_count++))
                            fi
                        done
                        echo ""
                        
                        # Limit output to prevent overwhelming
                        if [[ $group_count -ge 5 ]]; then
                            echo "  (ÊòæÁ§∫Ââç5ÁªÑÔºåÂèØËÉΩËøòÊúâÊõ¥Â§ö...)"
                            break
                        fi
                    fi
                    
                    # Start new group
                    current_size="$file_size"
                    size_files=("$file_path")
                else
                    size_files+=("$file_path")
                fi
            done < "$temp_dir/same_size.txt"
            
            # Process last group
            if [[ ${#size_files[@]} -gt 1 && $group_count -lt 5 ]]; then
                ((group_count++))
                local size_human=$(bytes_to_human $current_size)
                echo "üìÑ Áõ∏ÂêåÂ§ßÂ∞èÁªÑ #$group_count ($size_human):"
                for same_file in "${size_files[@]}"; do
                    echo "  ‚Ä¢ $(basename "$same_file")"
                    echo "    $same_file"
                done
                echo ""
            fi
            
            if [[ $duplicate_count -gt 1 ]]; then
                local waste_human=$(bytes_to_human $duplicate_size_saved)
                echo "üìä ÂèëÁé∞ $duplicate_count ‰∏™ÂèØËÉΩÈáçÂ§çÁöÑÊñá‰ª∂"
                echo "üíæ ÊΩúÂú®Êµ™Ë¥πÁ©∫Èó¥Ôºö$waste_human"
                echo ""
                echo "üí° Âª∫ËÆÆÔºö"
                echo "  ‚Ä¢ ÊâãÂä®Ê£ÄÊü•Áõ∏ÂêåÂ§ßÂ∞èÁöÑÊñá‰ª∂ÊòØÂê¶ÁúüÁöÑÈáçÂ§ç"
                echo "  ‚Ä¢ Âà†Èô§‰∏çÈúÄË¶ÅÁöÑÈáçÂ§çÊñá‰ª∂‰ª•ËäÇÁúÅÁ©∫Èó¥"
                echo "  ‚Ä¢ ‰ΩøÁî®‰∏ì‰∏öÂ∑•ÂÖ∑ËøõË°åÁ≤æÁ°ÆÁöÑÈáçÂ§çÊñá‰ª∂Ê£ÄÊµã"
            fi
            
        else
            echo "‚úÖ Êú™ÂèëÁé∞Áõ∏ÂêåÂ§ßÂ∞èÁöÑÊñá‰ª∂ÔºÅ"
            echo "üí° ÊÇ®ÁöÑÊñá‰ª∂ÁÆ°ÁêÜÂæàÂ•ΩÔºåÊ≤°ÊúâÊòéÊòæÁöÑÈáçÂ§çÊñá‰ª∂„ÄÇ"
        fi
    fi
    
    # Cleanup
    rm -rf "$temp_dir" 2>/dev/null
    
    echo ""
    echo "‚ö° Âø´ÈÄüÊ£ÄÊµãÂÆåÊàêÔºÅ"
    echo ""
    read -p "$(echo -e ${WHITE}$(get_text "press_enter")${NC})"
}

# System performance optimization function  
execute_performance_optimization() {
    print_title "‚ö° Á≥ªÁªüÊÄßËÉΩ‰ºòÂåñ"
    echo ""
    
    echo "üîß ÂàÜÊûêÁ≥ªÁªüÊÄßËÉΩ..."
    echo ""
    
    # Check system load
    local load_avg=$(uptime | awk -F'load averages:' '{print $2}' | awk '{print $1}' | tr -d ',')
    local cpu_count=$(sysctl -n hw.ncpu)
    local load_percent=$(echo "scale=0; $load_avg * 100 / $cpu_count" | bc 2>/dev/null || echo "0")
    
    echo "üìä Á≥ªÁªüÁä∂ÊÄÅÂàÜÊûêÔºö"
    echo "  CPU Ë¥üËΩΩÔºö${load_percent}% (${load_avg}/${cpu_count} cores)"
    
    # Memory analysis
    local memory_info=$(vm_stat | head -5)
    local free_pages=$(echo "$memory_info" | grep "Pages free" | awk '{print $3}' | tr -d '.')
    local inactive_pages=$(echo "$memory_info" | grep "Pages inactive" | awk '{print $3}' | tr -d '.')
    local page_size=$(vm_stat | grep "page size" | awk '{print $8}')
    
    if [[ -n "$free_pages" && -n "$page_size" ]]; then
        local free_memory=$(( (free_pages + inactive_pages) * page_size / 1024 / 1024 ))
        echo "  ÂèØÁî®ÂÜÖÂ≠òÔºö${free_memory}MB"
    fi
    
    # Disk space
    local disk_usage=$(df -h / | awk 'NR==2 {print $5}' | tr -d '%')
    echo "  Á£ÅÁõò‰ΩøÁî®Ôºö${disk_usage}%"
    
    echo ""
    echo "üöÄ ÊÄßËÉΩ‰ºòÂåñÂª∫ËÆÆÔºö"
    echo ""
    
    # Provide recommendations
    if [[ $load_percent -gt 80 ]]; then
        echo "  ‚ö†Ô∏è  È´òCPU‰ΩøÁî®ÁéáÊ£ÄÊµã"
        echo "     ‚Ä¢ ËÄÉËôëÈáçÂêØÈ´òË¥üËΩΩÂ∫îÁî®"
        echo "     ‚Ä¢ Ê£ÄÊü•Ê¥ªÂä®ÁõëËßÜÂô®‰∏≠ÁöÑËøõÁ®ã"
    fi
    
    if [[ $disk_usage -gt 85 ]]; then
        echo "  ‚ö†Ô∏è  Á£ÅÁõòÁ©∫Èó¥‰∏çË∂≥"
        echo "     ‚Ä¢ Âª∫ËÆÆËøêË°åËá™Âä®Ê∏ÖÁêÜÂäüËÉΩ"
        echo "     ‚Ä¢ Ê∏ÖÁêÜÂ§ßÊñá‰ª∂Âíå‰∏ãËΩΩÈ°π"
    fi
    
    if [[ -n "$free_memory" && $free_memory -lt 1024 ]]; then
        echo "  ‚ö†Ô∏è  ÂèØÁî®ÂÜÖÂ≠òËæÉ‰Ωé"
        echo "     ‚Ä¢ Âª∫ËÆÆÈáçÂêØ‰∏Ä‰∫õÂ∫îÁî®Á®ãÂ∫è"
        echo "     ‚Ä¢ ËÄÉËôëÊ∏ÖÁêÜÂÜÖÂ≠òÁºìÂ≠ò"
    fi
    
    echo "  ‚úÖ ÂèØÊâßË°åÁöÑ‰ºòÂåñÊìç‰ΩúÔºö"
    echo "     1. Ê∏ÖÁêÜDNSÁºìÂ≠ò"
    echo "     2. ÈáçÂª∫ÂêØÂä®ÊúçÂä°Êï∞ÊçÆÂ∫ì"
    echo "     3. Ê∏ÖÁêÜÂ≠ó‰ΩìÁºìÂ≠ò"
    echo "     4. Âà∑Êñ∞Á≥ªÁªüÂÜÖÂ≠ò"
    echo ""
    
    read -p "ÊòØÂê¶ÊâßË°åËøô‰∫õ‰ºòÂåñÊìç‰ΩúÔºü(y/N): " optimize_choice
    
    if [[ "$optimize_choice" =~ ^[Yy] ]]; then
        echo ""
        echo "üîß ÊâßË°åÊÄßËÉΩ‰ºòÂåñ..."
        
        # DNS cache flush
        echo "  üåê Âà∑Êñ∞DNSÁºìÂ≠ò..."
        sudo dscacheutil -flushcache 2>/dev/null && echo "    ‚úÖ DNSÁºìÂ≠òÂ∑≤Âà∑Êñ∞"
        
        # Launch services rebuild
        echo "  üöÄ ÈáçÂª∫ÂêØÂä®ÊúçÂä°Êï∞ÊçÆÂ∫ì..."
        /System/Library/Frameworks/CoreServices.framework/Frameworks/LaunchServices.framework/Support/lsregister -kill -r -domain local -domain system -domain user 2>/dev/null && echo "    ‚úÖ ÂêØÂä®ÊúçÂä°Êï∞ÊçÆÂ∫ìÂ∑≤ÈáçÂª∫"
        
        # Font cache
        echo "  üî§ Ê∏ÖÁêÜÂ≠ó‰ΩìÁºìÂ≠ò..."
        sudo atsutil databases -remove 2>/dev/null && echo "    ‚úÖ Â≠ó‰ΩìÁºìÂ≠òÂ∑≤Ê∏ÖÁêÜ"
        
        # Memory purge
        echo "  üíæ Ê∏ÖÁêÜÁ≥ªÁªüÂÜÖÂ≠ò..."
        sudo purge 2>/dev/null && echo "    ‚úÖ Á≥ªÁªüÂÜÖÂ≠òÂ∑≤Ê∏ÖÁêÜ"
        
        echo ""
        echo "üéâ ÊÄßËÉΩ‰ºòÂåñÂÆåÊàêÔºÅ"
        echo "üí° Âª∫ËÆÆÈáçÂêØÂ∫îÁî®Á®ãÂ∫è‰ª•Ëé∑ÂæóÊúÄ‰Ω≥ÊïàÊûú„ÄÇ"
    else
        echo "Ë∑≥Ëøá‰ºòÂåñÊìç‰Ωú„ÄÇ"
    fi
    
    echo ""
    read -p "$(echo -e ${WHITE}$(get_text "press_enter")${NC})"
}

